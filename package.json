    "@types/lodash-es": "4.17.12",
    "@types/ndarray": "^1.0.14",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.19",
    "@types/react-dom": "^18.3.5",
    "@types/three": "0.172.0",
    "@vitejs/plugin-legacy": "6.0.2",
    "@vitejs/plugin-react-swc": "3.8.1",
    "eslint": "9.23.0",
    "prettier": "3.5.3",
    "terser": "5.39.0",
    "typescript": "5.8.2",
    "vite": "6.2.3"
  },
  "pnpm": {
    "overrides": {
      "prop-types>react-is": "18.x"
    }

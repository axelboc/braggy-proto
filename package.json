    "@tanstack/react-query": "5.80.6",
    "d3-format": "3.1.0",
    "lodash-es": "4.17.21",
    "oidc-client-ts": "3.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "5.0.0",
    "react-icons": "5.5.0",
    "react-oidc-context": "3.3.0",
    "three": "0.172.0",
    "wouter": "3.6.0",
    "zustand": "5.0.3"
